# PSYBERHERDâ„¢ APEX Sniper - Vercel Frontend Environment Variables
# Copy this file to .env.local for local development

# Backend API Configuration
NEXT_PUBLIC_API_URL=https://psyberherd-apex-sniper.up.railway.app
NEXT_PUBLIC_WS_URL=wss://psyberherd-apex-sniper.up.railway.app

# Environment
NEXT_PUBLIC_ENVIRONMENT=production

# Feature Flags
NEXT_PUBLIC_ENABLE_QUANTUM_PROCESSING=true
NEXT_PUBLIC_ENABLE_AI_CONSENSUS=true
NEXT_PUBLIC_ENABLE_REAL_TIME_UPDATES=true
NEXT_PUBLIC_ENABLE_THREE_TIER_BACKUP=true

# Performance Settings
NEXT_PUBLIC_UPDATE_INTERVAL_MS=1000
NEXT_PUBLIC_RECONNECT_INTERVAL_MS=5000
NEXT_PUBLIC_MAX_RECONNECT_ATTEMPTS=10

# Security
NEXT_PUBLIC_API_KEY=your-api-key-here
NEXT_PUBLIC_ENCRYPTION_ENABLED=true

# Monitoring
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_ANALYTICS_ID=

# Multi-Platform Resilience
NEXT_PUBLIC_RAILWAY_STATUS_URL=https://psyberherd-apex-sniper.up.railway.app/health
NEXT_PUBLIC_VERCEL_STATUS_URL=https://psyberherd-apex.vercel.app/api/health
NEXT_PUBLIC_BACKUP_S3_REGION=us-east-1
NEXT_PUBLIC_BACKUP_STATUS_ENDPOINT=/api/backup/status

# Trading Configuration
NEXT_PUBLIC_DEFAULT_EXCHANGE=binance
NEXT_PUBLIC_DEFAULT_TIMEFRAME=1h
NEXT_PUBLIC_DEFAULT_LEVERAGE=10

# UI Configuration
NEXT_PUBLIC_THEME=dark
NEXT_PUBLIC_SHOW_ADVANCED_METRICS=true
NEXT_PUBLIC_ENABLE_ANIMATIONS=true